{
    "builds": [
        {
            "src": "/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/main.py"
        }
    ],
    "functions": {
    "/main.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}